{
    "name": "laraveladminpanel/admin",
    "description": "A Laravel Admin Package to make your life easier and steer your project in the right direction",
    "keywords": ["laravel", "admin", "panel"],
    "license": "MIT",
    "homepage": "http://laraveladminpanel.com",
    "support": {
        "issues": "http://laraveladminpanel.com/issues",
        "demo": "http://demo.laraveladminpanel.com/admin"
    },
    "authors": [
        {
            "name": "Alexey Kostenko",
            "email": "aleksey.kostenko.dev@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.4.0|~5.5.0|~5.6.0",
        "intervention/image": "^2.4",
        "doctrine/dbal": "^2.5",
        "larapack/doctrine-support": "~0.1.4",
        "arrilot/laravel-widgets": "^3.7",
        "league/flysystem": "~1.0.41",
        "yajra/laravel-datatables-oracle": "^8.6"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "dev-master",
        "phpunit/phpunit": "^6.4",
        "orchestra/database": "^3.5",
        "orchestra/testbench-browser-kit": "3.5",
        "phpunit/phpcov": "^4.0",
        "laravel/browser-kit-testing": "^2.0",
        "laravel/laravel": "^5.5",
        "symfony/css-selector": "3.1"
    },
    "autoload": {
        "psr-4": {
            "LaravelAdminPanel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelAdminPanel\\Tests\\": "tests/"
      }
    },
    "minimum-stability": "beta",
    "extra": {
        "laravel": {
            "providers": [
                "LaravelAdminPanel\\AdminServiceProvider"
            ]
        }
    }
}
